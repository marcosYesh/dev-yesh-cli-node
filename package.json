{
    "name": "dev-yesh-cli-node",
    "version": "1.0.11",
    "description": "A simple CLI tool for creating document structure in projects. It provides a straightforward interface for generating necessary files and folders. This tool is designed to assist in organizing project documents efficiently. It does not generate the content of the files, only the file structure.",
    "scripts": {
        "start": "node src/index.js init",
        "create-entity": "node src/index.js create-entity [entityName]",
        "create-component": "node src/index.js create-component [entityName] [componentType]",
        "help": "node src/index.js help"
    },
    "keywords": [
        "CLI",
        "structure",
        "document",
        "generator"
    ],
    "author": "Marcos <marcos@yesh.com.br>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/marcosYesh/dev-yesh-cli-node.git"
    },
    "bugs": {
        "url": "https://github.com/marcosYesh/dev-yesh-cli-node/issues"
    },
    "homepage": "https://github.com/marcosYesh/dev-yesh-cli-node#readme",
    "bin": {
        "dev-yesh-cli-node": "dev-yesh-cli-node.sh",
        "dev-yesh-cli-node.cmd": "dev-yesh-cli-node.bat"
    },
    "type": "module"
}
